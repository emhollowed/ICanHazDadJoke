@model CanIHazDadJoke.Models.JokeSearchResults

<form asp-action="Index" asp-controller="Home">
    <label asp-for="SearchTerm"></label>
    <input asp-for="SearchTerm" placeholder="type search term here" />
    <input type="submit" />
</form>


@if (Model != null)
{
    @if (Model.Results != null)
    {
        @foreach (var joke in Model.Results)
        {
            <li>@joke.Joke</li>
        }
    }
}





